{% extends 'base.html.twig' %}

{% block body %}
    <h1>{{ homepage_h1 }}</h1>
    {% for card in cards %}
        <div>
            <h2>{{ card.title }}</h2>
            <p>{{ card.description }}</p>
            <a href="{{ path('card', {'slug': card.slug}) }}">Go to {{ card.title }}</a>
        </div>
    {% endfor %}
{% endblock %}